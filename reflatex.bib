@article{kim_deeptfactor_2021,
	title = {{DeepTFactor}: {A} deep learning-based tool for the prediction of transcription factors},
	volume = {118},
	issn = {0027-8424, 1091-6490},
	shorttitle = {{DeepTFactor}},
	url = {http://www.pnas.org/lookup/doi/10.1073/pnas.2021171118},
	doi = {10.1073/pnas.2021171118},
	abstract = {A transcription factor (TF) is a sequence-specific DNA-binding protein that modulates the transcription of a set of particular genes, and thus regulates gene expression in the cell. TFs have commonly been predicted by analyzing sequence homology with the DNA-binding domains of TFs already characterized. Thus, TFs that do not show homologies with the reported ones are difficult to predict. Here we report the development of a deep learning-based tool, DeepTFactor, that predicts whether a protein in question is a TF. DeepTFactor uses a convolutional neural network to extract features of a protein. It showed high performance in predicting TFs of both eukaryotic and prokaryotic origins, resulting in
              F
              1 scores of 0.8154 and 0.8000, respectively. Analysis of the gradients of prediction score with respect to input suggested that DeepTFactor detects DNA-binding domains and other latent features for TF prediction. DeepTFactor predicted 332 candidate TFs in
              Escherichia coli
              K-12 MG1655. Among them, 84 candidate TFs belong to the y-ome, which is a collection of genes that lack experimental evidence of function. We experimentally validated the results of DeepTFactor prediction by further characterizing genome-wide binding sites of three predicted TFs, YqhC, YiaU, and YahB. Furthermore, we made available the list of 4,674,808 TFs predicted from 73,873,012 protein sequences in 48,346 genomes. DeepTFactor will serve as a useful tool for predicting TFs, which is necessary for understanding the regulatory systems of organisms of interest. We provide DeepTFactor as a stand-alone program, available at
              https://bitbucket.org/kaistsystemsbiology/deeptfactor
              .},
	language = {en},
	number = {2},
	urldate = {2021-05-31},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Kim, Gi Bae and Gao, Ye and Palsson, Bernhard O. and Lee, Sang Yup},
	month = jan,
	year = {2021},
	pages = {e2021171118},
	annote = {DeepTFactor: A deep learning-based tool for the prediction of transcription factors
Método:
Utilizar redes neurais convolucionais para a detecção e identificação de fatores de transcrição (TF).
Utilizam como input sequência de proteínas contendo TF e não-TF, realizam um one-hot encoding e três redes convolucionais paralelas com diversas camadas para a extração dos features e informações e mais duas camadas para o final da rede neural.

Dataset:
Realizam o treinamento e validação com dados Swiss-Prot em um período e para a comparação com outros métodos utilizam em outro período para uma comparação justa dos métodos.
Organismo Utilizado:
Aplicam o método no organismo da E.Coli para a identificação de novos TF
Resultado:
O método obteve resultados superiodos aos dois melhores métodos de predição de TF (tanto para procariotos como para eucariotos), possuindo uma acurácia acima de 98\% nas comparações.
Quando aplicado na E.Coli resultou na identificação de três novos TF.},
