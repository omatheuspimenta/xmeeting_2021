@article{bailey_streme_2021,
	title = {{STREME}: accurate and versatile sequence motif discovery},
	issn = {1367-4803, 1460-2059},
	shorttitle = {{STREME}},
	url = {https://academic.oup.com/bioinformatics/advance-article/doi/10.1093/bioinformatics/btab203/6184861},
	doi = {10.1093/bioinformatics/btab203},
	abstract = {Abstract
            
              Motivation
              Sequence motif discovery algorithms can identify novel sequence patterns that perform biological functions in DNA, RNA and protein sequences—for example, the binding site motifs of DNA- and RNA-binding proteins.
            
            
              Results
              The STREME algorithm presented here advances the state-of-the-art in ab initio motif discovery in terms of both accuracy and versatility. Using in vivo DNA (ChIP-seq) and RNA (CLIP-seq) data, and validating motifs with reference motifs derived from in vitro data, we show that STREME is more accurate, sensitive and thorough than several widely used algorithms (DREME, HOMER, MEME, Peak-motifs) and two other representative algorithms (ProSampler and Weeder). STREME’s capabilities include the ability to find motifs in datasets with hundreds of thousands of sequences, to find both short and long motifs (from 3 to 30 positions), to perform differential motif discovery in pairs of sequence datasets, and to find motifs in sequences over virtually any alphabet (DNA, RNA, protein and user-defined alphabets). Unlike most motif discovery algorithms, STREME reports a useful estimate of the statistical significance of each motif it discovers. STREME is easy to use individually via its web server or via the command line, and is completely integrated with the widely used MEME Suite of sequence analysis tools. The name STREME stands for ‘Simple, Thorough, Rapid, Enriched Motif Elicitation’.
            
            
              Availability and implementation
              The STREME web server and source code are provided freely for non-commercial use at http://meme-suite.org.
            
            
              Supplementary information
              Supplementary data are available at Bioinformatics online.},
	language = {en},
	urldate = {2021-05-31},
	journal = {Bioinformatics},
	author = {Bailey, Timothy L},
	editor = {Birol, Inanc},
	month = mar,
	year = {2021},
	pages = {btab203},
	annote = {STREME: accurate and versatile sequence motif discovery
Método:
Busca descobrir os motifs nas sequencias. Seis passos o algortimo:
1 etapa: preparação do dataset    realiza uma 'normalização' dos dados deixando tudo maiusculo, e caso não tenha uma sequência padrão de entrada é gerada uma sequência através de um suffle na sequencia de entrada no qual preserva. Através dessas sequencias de entrada é definido o teste estatístico a ser tomado para a determinação da significancia estatística.
2 etapa: criação da arvore de sufixo    são criadas arvores para as duas sequencias de entrada baseada no algortido de MUMmer, caso as letras tenham complementos são incluidos
3 etapa: avaliação das palavras    são geradas as palavras de tamanho 3 até o maior valor e são identificadas a existencia na arvore
4 etapa: refinamento dos motifs    cada palavra é convertida em uma matriz de pesos e testes estatisticos são feitos
5 etapa: siginificancia dos motifs    é computada a significancia estatistica de cada um dos motifs descobertos
6 etapa: exclusão de motifs    os melhores motifs são apagados e substituidos por caracteres de separação
Dataset:
40 TF ChIP-seq for Homo sapiens
Organismo Utilizado:
Homo sapiens
Resultado:
Avaliações e comparações com outros métodos de descoberta de motifs mostraram que o STREME possui um tempo computacional baixo para a descoberta de motifs e acertividade superior aos demais métodos.
Além disso, STREME fornece significancia estatística para os motifs descobertos, apresentando um diferencial entre os demais métodos.},
	file = {btab203.pdf:/home/matheus/Zotero/storage/DVRAAX6E/btab203.pdf:application/pdf;Versão submetida:/home/matheus/Zotero/storage/IWY5J2P9/Bailey - 2021 - STREME accurate and versatile sequence motif disc.pdf:application/pdf},
}

