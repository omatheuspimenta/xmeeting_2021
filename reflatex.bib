@article{gligorijevic_structure-based_2021,
	title = {Structure-based protein function prediction using graph convolutional networks},
	volume = {12},
	issn = {2041-1723},
	url = {http://www.nature.com/articles/s41467-021-23303-9},
	doi = {10.1038/s41467-021-23303-9},
	abstract = {Abstract
            
              The rapid increase in the number of proteins in sequence databases and the diversity of their functions challenge computational approaches for automated function prediction. Here, we introduce DeepFRI, a Graph Convolutional Network for predicting protein functions by leveraging sequence features extracted from a protein language model and protein structures. It outperforms current leading methods and sequence-based Convolutional Neural Networks and scales to the size of current sequence repositories. Augmenting the training set of experimental structures with homology models allows us to significantly expand the number of predictable functions. DeepFRI has significant de-noising capability, with only a minor drop in performance when experimental structures are replaced by protein models. Class activation mapping allows function predictions at an unprecedented resolution, allowing site-specific annotations at the residue-level in an automated manner. We show the utility and high performance of our method by annotating structures from the PDB and SWISS-MODEL, making several new confident function predictions. DeepFRI is available as a webserver at
              https://beta.deepfri.flatironinstitute.org/
              .},
	language = {en},
	number = {1},
	urldate = {2021-05-31},
	journal = {Nature Communications},
	author = {Gligorijević, Vladimir and Renfrew, P. Douglas and Kosciolek, Tomasz and Leman, Julia Koehler and Berenberg, Daniel and Vatanen, Tommi and Chandler, Chris and Taylor, Bryn C. and Fisk, Ian M. and Vlamakis, Hera and Xavier, Ramnik J. and Knight, Rob and Cho, Kyunghyun and Bonneau, Richard},
	month = dec,
	year = {2021},
	pages = {3168},
	annote = {Structure-based protein function prediction using graph convolutional networks
Método:
Utilizam deep learning para predizer a função da proteína classificada através do GO. A contribução está na estrutura da da rede neural considerada. São consideradas duas etapas da rede neural.
Para a extração de features dos dados de entrada é utilizada uma rede neural com memoria long short-term, utilizada em sua maioria para a aprendizagem de processos NLP, pré-treinada com 10 milhões de entradas de proteínas.

The first stage of our method is a self-supervised language model with a recurrent neural network architecture with long short-term memory (LSTM-LM)50.

Após a extração dos features, uma rede neural convolucional baseada em grafo é utilizada contendo duas camadas. A entrada de valores para as redes neurais é a saída da rede neural anterior, devido a isso os features são extraídos na camada anterior.



O esquema resumido da rede neural é apresentado acima. Além das informações sobre o grafo gerado pelas ligações dos aminoácidos também são incluídas informações, informações sobre as ligações dos aminoácidos obtidas através da rede neural Short-term-memory. Isso contribui para a acertividade da rede neural e também para a seleção de "partes" que contribuem para a predição de funções desempenhada pela proteína.
Em resumo, são incluídas as informações sobre a "linguagem" da proteína e também de sua estrutura. Para a extração dos features da "linguagem" da proteína é utilizada uma rede neural inicial e a inclusão da estrutura da proteína ocorre utilizando rede neural convolucional baseada em grafos, através da representação das proteínas por mapas de contato e matrizes de adjacencia. São testados diversos tipos de camadas de redes neurais convolucionais baseadas em grafos até a seleção da Graph Attention (GAT). 
Outras informações estão no artigo.
Dataset:
Banco de dados de proteínas
PDB: Protein Data Bank
Swiss-Model: subconjunto de proteínas curadas manualmente do PDB.
Além disso utilizam informações biológicas através
GO: Gene Ontology
EC: Enzyme Commission
Organismo Utilizado:
São proteínas de diversos organismos.
Resultado:
Ao ser comparado com os outros métodos disponíveis para a predição de função biológica (DeepGO e BLAST) o método obteve resultados promissores, sobresaindo-se em algumas comparações, a depender do threshold tomado. Para a predição de função biológica houve um grande acertividade.
Além da predição de função GO para a proteína, também foram obtidos resultados (que não são o principal objetivo do método) em relação a mapear locais funcionais nas proteínas.},
	file = {Texto completo:/home/matheus/Zotero/storage/FD92NM5I/Gligorijević et al. - 2021 - Structure-based protein function prediction using .pdf:application/pdf},
}

@article{lemoine_gwena_2021,
	title = {{GWENA}: gene co-expression networks analysis and extended modules characterization in a single {Bioconductor} package},
	volume = {22},
	issn = {1471-2105},
	shorttitle = {{GWENA}},
	url = {https://bmcbioinformatics.biomedcentral.com/articles/10.1186/s12859-021-04179-4},
	doi = {10.1186/s12859-021-04179-4},
	abstract = {Abstract
            
              Background
              Network-based analysis of gene expression through co-expression networks can be used to investigate modular relationships occurring between genes performing different biological functions. An extended description of each of the network modules is therefore a critical step to understand the underlying processes contributing to a disease or a phenotype. Biological integration, topology study and conditions comparison (e.g. wild vs mutant) are the main methods to do so, but to date no tool combines them all into a single pipeline.
            
            
              Results
              Here we present GWENA, a new R package that integrates gene co-expression network construction and whole characterization of the detected modules through gene set enrichment, phenotypic association, hub genes detection, topological metric computation, and differential co-expression. To demonstrate its performance, we applied GWENA on two skeletal muscle datasets from young and old patients of GTEx study. Remarkably, we prioritized a gene whose involvement was unknown in the muscle development and growth. Moreover, new insights on the variations in patterns of co-expression were identified. The known phenomena of connectivity loss associated with aging was found coupled to a global reorganization of the relationships leading to expression of known aging related functions.
            
            
              Conclusion
              
                GWENA is an R package available through Bioconductor (
                https://bioconductor.org/packages/release/bioc/html/GWENA.html
                ) that has been developed to perform extended analysis of gene co-expression networks. Thanks to biological and topological information as well as differential co-expression, the package helps to dissect the role of genes relationships in diseases conditions or targeted phenotypes. GWENA goes beyond existing packages that perform co-expression analysis by including new tools to fully characterize modules, such as differential co-expression, additional enrichment databases, and network visualization.},
	language = {en},
	number = {1},
	urldate = {2021-05-31},
	journal = {BMC Bioinformatics},
	author = {Lemoine, Gwenaëlle G. and Scott-Boyer, Marie-Pier and Ambroise, Bathilde and Périn, Olivier and Droit, Arnaud},
	month = dec,
	year = {2021},
	pages = {267},
	annote = {GWENA: gene co-expression networks analysis and extended modules characterization in a single Bioconductor package
Método:
Busca fornecer um pipeline completo para inferência de rede de co-expressão gênica.
Gene Whole co-Expression Network Analysis (GWENA) agrega diversos outros algoritmos para a análise de redes gênicas. São diversas etapas do pipeline:
Etapa 1: Entrada dos dados
    Pode ser uma matriz de microarray ou dados de RNA-seq, desde que possua formato matricial e que as colunas sejam genes e as linhas experimentos. Recomenda-se realizar uma normalização dos dados para a entrada e também um mínimo de     20 experimentos por entrada para melhores resultados.
Etapa 2: Filtro
    Busca reduzir o efeito do ruído presente nos experimentos e evitar informações falsas, são fornecidos dois tipos de filtros:        Filtro de contagem baixa: remove genes com baixa expressão baseado em um threshold        Filtro de pouca variação: remove genes cuja variação de uma amostra para outra é baixa ou insignificante, ou seja não há variação biológica significantes.
Etapa 3: Construção da rede de co-expressão gênica
    Nesta etapa é utilizada o pacote WGCNA para a elaboração da matriz de correlação (Spearman ou Pearson), modelagem para a lei de potencia (scale-free topology) e matriz topológica da rede, após esses passos é fornecida a matriz de     adjacência para a rede gênica.
Etapa 4: Detecção de módulos
    Utilizando a biblioteca WGCNA é fornecida uma clusterização hierárquica dos genes utilizando o PCA (principal component analysis) e os genes que mais contribuem para o módulo são selecionados.
Etapa 5: Integração biológica
    É realizada um enriquecimento biológico e uma associação do fenótipo aos genes. Para o enriquecimento biológico 9 banco de dados são pesquisados e utilizados para o enriquecimento. Para a análise dos fenótipos são utilizados os genes     selecionados na etapa anterior (etapa 4).
Etapa 6: Análise topológica da rede gerada
    Utilizando a biblioteca igraph são geradas medidas topológicas para a rede gênica em estudo, em particular a presença de hubs. Para isso são utilizadas três medidas: maior conectividade, maior grau do vértice e score de Kleinberg. Outros     submódulos podem ser analisados separadamente nesta etapa.
Etapa 7: Co-expressão diferencial dos módulos
    São obtidas 7 métricas relacionadas a topologia do módulo e aleatóriamente são modificados genes e realizado um testes de hipótese entre as mudanças e a observação, um módulo será considerado preservado se for significativo para a     hipótese alternativa contudo caso não seja, um segundo teste é realizado buscando o Z-score, a combinação destas duas etapas fornece um valor final de score para os módulos.
Dataset:
O pipeline é testado utilizando RNA-seq skeletal muscle data from GTEx.
Organismo Utilizado:
Humano através de dados de RNA-seq.
Resultado:
São realizadas duas análises, a primeira em relação ao dataset utilizado e a segunda para comparação com outros pipelines disponíveis.
A inferência para o dataset é feita dividindo as amostras em "jovens" e "idosos" e busca-se avaliar as diferenças entre a expressão gênica com o passar do tempo, para isso são executadas todas as etapas do método com o objetivo de mostrar todo o potencial analítico.
A comparação do método com os demais pipelines é feita através de uma tabela apresentando as funcionalidades disponíveis, a grande diferença do método é a grande gama de análises disponíveis tudo integrado ao pipeline.
Como limitações são apresentadas: qualidade limitada dos dados de entrada e o possível viés disponível fazendo com que os dados não sigam topologia scale-free, a limitação em relação ao teste de hipótese e as hipóteses, fazendo com que a tendência dos módulos sejam de não-preservação.},
	file = {Texto completo:/home/matheus/Zotero/storage/X9BXVYT9/Lemoine et al. - 2021 - GWENA gene co-expression networks analysis and ex.pdf:application/pdf},
}

